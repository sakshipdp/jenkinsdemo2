pipeline {
    agent any
    environment {
        PYTHON = 'C:\\Users\\Dell\\AppData\\Local\\Programs\\Python\\Python314\\python.exe'
    }
    stages {
        stage('checkout code') {
            checkout scm
        }
        stage('extract data') {
            bat "${env.PYTHON} extract.py" 
        }
        
    }

    post {
        success {
           echo "success...."
        }
        failure {
           echo "failure...."
        }
        always {
           echo "always...."
        }
    }
}